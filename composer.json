{
  "name": "lednerb/text-linked",
  "description": "Laravel Nova custom Text Field with a link to a resource or custom URL",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Lednerb\\TextLinked\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lednerb\\TextLinked\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
